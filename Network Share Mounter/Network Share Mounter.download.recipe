            <key>NAME</key>
            <string>Network Share Mounter</string>
            <key>APP_NAME</key>
            <string>%NAME%.app</string>
            <key>RELEASE_CHANNEL</key>
            <string>release</string> <!-- release or beta -->
        </dict>
        <key>MinimumVersion</key>
        <string>0.4.1</string>
        <key>Process</key>
        <array>
        <dict>
            <key>Processor</key>
            <string>URLTextSearcher</string>
            <key>Arguments</key>
            <dict>
                <key>url</key>
                <string>https://gitlab.rrze.fau.de/api/v4/projects/506/releases</string>
                <key>re_pattern</key>
                <string>%RELEASE_CHANNEL%-((\d(\.)*)+([a-z|0-9]+){0,})</string>
                <key>result_output_var_name</key>
                <string>match</string>
            </dict>
        </dict>
        <dict>
            <key>Processor</key>
            <string>URLTextSearcher</string>
            <key>Arguments</key>
            <dict>
                <key>url</key>
                <string>https://gitlab.rrze.fau.de/api/v4/projects/506/releases</string>
                <key>re_pattern</key>
                <string>%match%/NetworkShareMounter-%match%((([a-z]|[A-Z])+){0,}).dmg</string>
                <key>result_output_var_name</key>
                <string>match2</string>
            </dict>
        </dict>
        <dict>
            <key>Processor</key>
            <string>URLDownloader</string>
            <key>Arguments</key>
